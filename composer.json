{
    "name": "jngermon/dotfils-installer",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-NoÃ«l Germon",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^5.5.9||^7.0",
        "incenteev/composer-parameter-handler": "~2.1",
        "symfony/config": "~3.1",
        "symfony/console": "~3.1",
        "symfony/dependency-injection": "~3.1",
        "symfony/yaml": "^3.1",
        "meuhmeuhconcept/processor": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "scripts": {
        "post-install-cmd": [
             "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "autoload": {
        "psr-4": {
            "DotfilesInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotfilesInstaller\\Test\\": "tests/"
        }
    },
    "bin": ["bin/dotfiles-installer"],
    "extra": {
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
